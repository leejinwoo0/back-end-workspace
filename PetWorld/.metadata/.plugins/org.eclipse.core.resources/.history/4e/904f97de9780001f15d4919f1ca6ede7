<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.PetsitterMapper">
<resultMap id="petsitterMap" type="Petsitter">
<id column="serviceman_code" property="servicemanCode"/>
<result column="serviceman_class" property="servicemanClass"/>
<result column="serviceman_phone" property="servicemanPhone"/>
<result column="price" property="price"/>
</resultMap>
</mapper>

CREATE TABLE PetSitter (
serviceman_code INT PRIMARY KEY,
serviceman_class VARCHAR(50),
serviceman_phone VARCHAR(15),
price DECIMAL (10, 2)
);